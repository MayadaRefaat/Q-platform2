@model IEnumerable<mr_bahaa.Models.onelecture>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-sm-9"><h2>المحاضرات </h2></div>

<p>
</p>
<table class="table">
    <tr>
        <th>
            @Html.Label("اسم المحاضره")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.lecture.name)
        </td>
        <td>
            |
        </td>
        <td>
            <button class="btn btn-primary back py-2"> @Html.ActionLink("view", "view", "onelectures", new { id = item.oneid }, null)  </button>
        </td>
    </tr>
}

</table>
